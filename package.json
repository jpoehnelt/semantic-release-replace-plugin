{
  "name": "@jpoehnelt/semantic-release-replace-plugin",
  "version": "1.2.0",
  "description": "Semantic Release Replace Plugin",
  "keywords": [
    "semantic-release",
    "node",
    "replace",
    "google"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jpoehnelt/semantic-release-replace-plugin.git"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "Justin Poehnelt",
    "email": "justin.poehnelt@gmail.com"
  },
  "main": "dist/index.js",
  "files": [
    "dist/*",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "all": "npm run build && npm run lint && npm test && npm run docs",
    "build": "tsc",
    "docs": "rm -rf docs && typedoc src/index.ts",
    "lint": "eslint src/**/*.ts",
    "format": "npm run lint -- --fix",
    "test": "tsc --noEmit && vitest --coverage",
    "release": "semantic-release"
  },
  "dependencies": {
    "jest-diff": "^26.5.2",
    "lodash-es": "^4.17.21",
    "replace-in-file": "^6.3.5"
  },
  "devDependencies": {
    "@semantic-release/commit-analyzer": "^10.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.0.3",
    "@semantic-release/npm": "^10.0.4",
    "@semantic-release/release-notes-generator": "^11.0.4",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^14.11.2",
    "@types/semantic-release": "^17.1.0",
    "@types/tmp": "^0.2.3",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "@vitest/coverage-c8": "^0.28.5",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "fs-extra": "^9.0.1",
    "prettier": "^2.8.1",
    "semantic-release": "^21.0.7",
    "tmp": "^0.2.1",
    "typedoc": "^0.23.23",
    "typedoc-plugin-markdown": "^3.14.0",
    "typescript": "^4.9.4",
    "vitest": "^0.26.1"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://wombat-dressing-room.appspot.com"
  }
}
